QMainWindow {
    background-color: "#ffffff";
}

QLineEdit#lineEdit_AddingItem {
    color: "#000000";
    font-family: 'Roboto', arial, cursive;
    font-size: 14px;
    background-color: "#e3e4e6";
    border-radius: 2px;
    padding-left: 5px;
    height: 30px;
}

QLineEdit#lineEdit_SearchItem {
    color: "#000000";
    font-family: 'Roboto', arial, cursive;
    background-color: "#e3e4e6";
    border-radius: 2px;
    padding-left: 5px;
    height: 30px;
}

QListWidget {
    color: "#1b1b1b";
    font-family: 'Roboto', arial, cursive;
    background-color: "#e3e4e6";
    border-radius: 4px
}

QListWidget QLabel {
    color: "#1b1b1b";
    font-family: 'Segoe UI', sans-serif;
    font-size: 18px;
    border-radius: 4px
}

QListWidget QPushButton {
    background-color: transparent;
}

QListWidget QPushButton:hover {
    background-color: transparent;
}

QPushButton {
    background-color: "#e3e4e6";
    padding-top: 6px;
    padding-bottom: 6px;
    border-radius: 10px
}

QPushButton:hover {
    background-color: "#cfd0d3";
}

QPushButton#pushButton_Find {
    padding: 7px
}

QPushButton#pushButton_Create {
    padding-right: 30px;
    padding-left: 30px;
    background-color: "#3b82f6";
}

QPushButton#pushButton_Create:hover {
    background-color: "#2563eb";
}

QMessageBox {
    background-color: "#ffffff";
    font-family: 'Roboto', arial, cursive;
    font-size: 16px;
}

QMessageBox QLabel {
    color: "#000000";
}

QMessageBox QPushButton {
    background-color: "#e3e4e6";
    font-weight: bold;
    border-radius: 10px;
    padding: 5px
}

QMessageBox QPushButton:hover {
    background-color: "#cfd0d3";
}

QInputDialog {
    background-color: "#f2f2f7";
    color: "#ffffff";
    font-family: 'Roboto', arial, cursive;
}

QInputDialog QLabel {
    color: "#1b1b1b";
    font-family: 'Roboto', arial, cursive;
    font-size: 15px;
}

QInputDialog QLineEdit {
    color: "#1b1b1b";
    font-size: 15px;
    background-color: "#e3e4e6";
    border-radius: 2px;
    height: 30px;
}

QInputDialog QPushButton {
    background-color: "#e3e4e6";
    font-family: 'Roboto', arial, cursive;
    font-weight: bold;
    border-radius: 10px;
    padding: 5px;
}

QInputDialog QPushButton:hover {
    background-color: "#cfd0d3";
    font-family: 'Roboto', arial, cursive;
}